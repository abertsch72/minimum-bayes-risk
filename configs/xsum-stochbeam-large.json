{
    "pipeline": {
        "hf_model_name": "facebook/bart-large-xsum",
        "hf_tokenizer_name": null,
        "wandb": false,
        "wandb_group": "xsum",
        "run_name": "xsum-bart-large-beam",
        "seed": 14,
        "no_tqdm": false,
        "no_gpu": false
    },
    "dataset": {
        "dataset": "xsum",
        "split": "val",
        "start_index": 0,
        "end_index": 1000,
        "shuffle": true
    },
    "gen": {
        "method_args": {
            "num_beams": 50,
            "stochastic": true,
            "memoryless": false,
            "beam_temp": 0.7
        },
        "outfile": null,
        "k": 50,
        "max_length": 35
    },
    "rerank": {
        "rerank_metric": "rouge1",
        "rerank_temp": Infinity,
        "rerank_geo_mean": false
    },
    "eval": {
        "eval_metrics": "rouge1,rouge2,rougeL,chrf,bertscore",
        "outfile": null
    }
}
